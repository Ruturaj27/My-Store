var clientDateTimeValidation = Class.create();
clientDateTimeValidation.prototype = Object.extendsObject(AbstractAjaxProcessor, {
	
	getScheduledDueDate : function(){
		
		var startDate = this.getParameter('sysparm_sdt');
		var endDate = this.getParameter('sysparm_edt');
		var dt = new GlideDateTime(startDate);
		var et = new GlideDateTime(endDate);
		var days = 3;
		var dur = new GlideDuration(60 * 60 * 24 * 1000 * days);
		var schedule = new GlideSchedule(gs.getProperty('rms.ss.support.schedule.sysid'));
		var end = schedule.add(dt, dur);
		var sch = new GlideDateTime(end);
		var diffSeconds = gs.dateDiff(sch.getDisplayValue(), et.getDisplayValue(), true);
		
		//if(diffSeconds <= 259200){
			return diffSeconds;
			
			// 		}
			// 		else{
				
				// 			return false;
				// 		}
				
			},
			type: 'clientDateTimeValidation'
		});
